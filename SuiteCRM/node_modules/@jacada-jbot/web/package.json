{
    "name": "@jacada-jbot/web",
    "version": "4.0.6",
    "description": "Standard Web Coverage Handler using JQuery",
    "directories": {},
    "scripts": {
        "build-dev": "npm run build-dev --prefix ./node_modules/@jacada-jbot/coverage-utils && node src/Utils/CopyJQueryExtensions.js",
        "build-prod": "npm run build-prod --prefix ./node_modules/@jacada-jbot/coverage-utils && node src/Utils/CopyJQueryExtensions.js",
        "create-zip": "npm run create-zip --prefix ./node_modules/@jacada-jbot/coverage-utils",
        "build-docs": "npx typedoc --theme ../docs_theme/ src/ node_modules/@jacada-jbot/base/include/ --disableSources --excludePrivate --excludeInternal"
    },
    "keywords": [],
    "author": "Jacada",
    "license": "ISC",
    "files": [
        "elementsMetadata/elementsMetadata.json",
        "include/"
    ],
    "main": "./dist/coverage.js",
    "coverageType": "Framework",
    "dependencies": {
        "@jacada-jbot/base": "^4.0.0",
        "@jacada-jbot/coverage-utils": "^1.0.8",
        "@types/node": "^14.14.41",
        "core-js": "^3.11.0",
        "jquery": "^3.6.0",
        "typescript-logging": "^0.6.4",
        "underscore": "^1.13.1"
    },
    "devDependencies": {
        "@types/chrome": "0.0.100",
        "@types/jquery": "^3.5.6",
        "copy-webpack-plugin": "^6.4.1",
        "ts-loader": "^6.2.1",
        "typescript": "^4.3.5"
    }
}