{
  "name": "@jacada-jbot/coverage-utils",
  "version": "1.0.8",
  "description": "Utilities needed in all coverages",
  "main": "index.js",
  "scripts": {
    "build-dev": "cd ../../.. && node ./node_modules/@jacada-jbot/coverage-utils/deleteFiles.js && tsc && webpack --mode=development --progress --devtool inline-cheap-source-map && node ./node_modules/@jacada-jbot/coverage-utils/copyFiles.js",
    "build-prod": "cd ../../.. && node ./node_modules/@jacada-jbot/coverage-utils/deleteFiles.js && tsc && webpack --mode=production --progress --optimize-minimize --hide-modules && node ./node_modules/@jacada-jbot/coverage-utils/copyFiles.js",
    "create-zip": "cd ../../.. && node ./node_modules/@jacada-jbot/coverage-utils/createZip.js"
  },
  "author": "Jacada",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
